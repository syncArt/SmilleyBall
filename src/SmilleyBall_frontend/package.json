    "@testing-library/react": "^16.0.0",
    "@vitest/ui": "^2.0.5",
    "jsdom": "^25.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vitest": "^2.0.5"
  },
  "devDependencies": {
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vitejs/plugin-react": "^4.0.1",
    "dotenv": "^16.3.1",
    "eslint": "^9.9.1",
